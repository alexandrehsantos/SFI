//Modulo Principal 
var appListas = angular.module("appListas",['ngRoute']);



appListas.config(function($routeProvider) {
	$routeProvider
		.when("/listas", {templateUrl:'view/lista.html', controller:'listaController'})
		.otherwise({redirectTo:'/'});
});


$locationProvider.html5Mode({
    enabled: true,
